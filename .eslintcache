serProfile,\r\n        userToken: state.user.userToken,\r\n        auth: state.ui.authenticated\r\n    }\r\n}\r\n\r\nconst mapDispatchToProps = dispatch => {\r\n    return {\r\n        getUserData: (userData) => dispatch(storeUserData(userData)),\r\n        //userProfile: (profile) => dispatch(userProfile(profile)),\r\n        checkAuth: () => dispatch(checkAuthentication()),\r\n        //getUserToken: (userToken) => dispatch(storeUserToken(userToken))\r\n    }\r\n}\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(Login);","C:\\Users\\Hwang Jae Soon\\Desktop\\english-didary\\src\\component\\UI\\Spinner\\Spinner.js",[],"C:\\Users\\Hwang Jae Soon\\Desktop\\english-didary\\src\\component\\UI\\Dropdown\\Dropdown.js",["147"],"import React from 'react';\r\nimport styled from 'styled-components';\r\nimport { Link, Redirect } from 'react-router-dom';\r\n\r\nconst StyledDropdown = styled.ul`   \r\n    .userUI:focus-within & {\r\n        max-height: 20rem;\r\n    }    \r\n\r\n    background: white;\r\n    position: absolute;\r\n    right: 0;\r\n    top: 0;\r\n    overflow: hidden;\r\n    margin-top: 4.1rem;\r\n    z-index: 40;\r\n    max-height: 0;\r\n    width: 8rem;\r\n    transition: all 0.2s ease-in;\r\n    border-radius: 10px;\r\n    box-shadow: 0 1px 5px rgba(0, 0, 0, 0.3);\r\n\r\n    & li {\r\n        height: 2.5rem;\r\n    }\r\n\r\n    & button {\r\n        cursor: pointer;\r\n        width: 100%;\r\n        height: 100%;\r\n        background-color: transparent;\r\n        border: none;\r\n        font-size: 0.7rem;\r\n    }\r\n\r\n    & button:hover {\r\n        background: #0366d6;\r\n        color: white;\r\n    }\r\n\r\n    @media (max-width: 599px) {\r\n        margin-top: 3.1rem;\r\n    }\r\n\r\n`\r\n\r\nconst dropdown = props => {\r\n    return (\r\n        //<StyledDropdown className={props.toggleDropdown ? 'toggle' : null}>\r\n        <StyledDropdown>\r\n            <li>\r\n                <Link to='profile'>\r\n                    <button 